<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="hong" class="sample5.DateVo">
<!-- setter 메서드 -->
	<property name="name" value="Duke"/>
	<property name="birth" value="1990-01-01"/>
</bean>
<!-- property -> p(prefix) 프리픽스의 경우에는 미리 선언부에 선언이 되어 있어야한다.  -->
<bean id="lee" class="sample5.DateVo" p:name="Dooly"  p:birth="2000-12-12"/>	
</beans>

 
 